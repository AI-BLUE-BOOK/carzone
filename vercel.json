{
    "scripts": {

  "prebuild": "pip install -r requirements.txt",

  "build": "vercel build"

    },

    "builds": [{
      "src": "carzone/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.8.7" }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "carzone/wsgi.py"
      }
    ]
}